# /etc/cron.d/fjarrkontrollen_server_production_cron_script

# Update fjarrkontrollen db every 30 minutes
*/30 * * * *     apps /apps/fjarrlan/fjarrkontrollen-server/current/libris_neg_responses.sh production /apps/fjarrlan/fjarrkontrollen-server/current >> /apps/fjarrlan/fjarrkontrollen-server/logs/cronlog.txt 2>&1
*/30 * * * *     apps /apps/fjarrlan/fjarrkontrollen-server/current/libris_user_requests.sh production /apps/fjarrlan/fjarrkontrollen-server/current >> /apps/fjarrlan/fjarrkontrollen-server/logs/cronlog.txt 2>&1
00 01  * * *     apps /apps/fjarrlan/fjarrkontrollen-server/current/export_orders.sh production /apps/fjarrlan/fjarrkontrollen-server/current >> /apps/fjarrlan/fjarrkontrollen-server/logs/cronlog.txt 2>&1